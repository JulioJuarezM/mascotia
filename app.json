{
  "expo": {
    "name": "Mascotia",
    "slug": "mascotia",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/images/icon.png",
    "scheme": "mascotia",
    "userInterfaceStyle": "automatic",
    "splash": {
      "image": "./assets/images/icon.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff",
      "hideExponentIcon": true
    },
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.anonymous.mascotia",
      "buildNumber": "1"
    },
    "web": {
      "bundler": "metro",
      "output": "single",
      "favicon": "./assets/images/favicon.png"
    },
    "plugins": [
      "expo-router"
    ],
    "experiments": {
      "typedRoutes": true
    },
    "android": {
      "package": "com.anonymous.mascotia"
    }
  }
}
